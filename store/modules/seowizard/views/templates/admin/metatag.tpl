<div class="velsof_overlay"></div>
<div class="row" id="kbgc_image_create_block">
    <div class="col-sm-12">
        <div class="panel">
            <div class="image_form_container">
                {$meta_form} {* Variable contains HTML content, escape not required *}
                <div class="knowband_meta_default_fields">
                    <b>{l s='For product groups' mod='seowizard'} </b><br>
                    [product_name]
                    <br>
                    [product_description]
                    <br>
                    [product_short_description]
                    <br>
                    [product_reference]
                    <br>
                    [product_manufacturer]
                    <br>
                    [product_category_name]
                    <br>
                    <b>{l s='For category groups' mod='seowizard'} </b><br>
                    [category_name]
                    <br>
                    [category_description]
                    <br>
                    [category_short_description]
                    <b><br>{l s='For Manufacturers groups' mod='seowizard'} </b><br>
                    [manufacturer_name]
                    <br>
                    [manufacturer_description]
                    <br>
                    [manufacturer_short_description]
                    <br>
                </div>
            </div>
        </div>        
    </div>
</div>


